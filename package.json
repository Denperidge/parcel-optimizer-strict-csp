  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "parcel",
    "inline script",
    "CSP",
    "Content Security Policy"
  ],
  "author": {
    "name": "Henrik Joreteg",
    "email": "henrik@joreteg.com",
    "url": "https://joreteg.com"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "7.18.0",
    "parcel": "2.0.0",
    "prettier": "2.2.1",
    "tape": "5.0.1"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.10"
  }
