    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@types/node": "^22.5.5",
    "eslint": "7.18.0",
    "parcel": "2.0.0",
    "prettier": "2.2.1",
    "tape": "5.0.1",
    "typescript": "^5.6.2"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "dependencies": {
